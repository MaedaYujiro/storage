@article{10.3389/fphys.2018.00646,
  author   = {Naldi, Aurélien  and Hernandez, Céline  and Abou-Jaoudé, Wassim  and  Monteiro, Pedro T.  and Chaouiya, Claudine  and Thieffry, Denis },
  title    = {Logical Modeling and Analysis of Cellular Regulatory Networks With GINsim 3.0},
